{
  "author": "Michael Mykhaylov <mikemikhailov2003@gmail.com>",
  "description": "React Boilerplate made on Snowpack, TypeScript and styled-components",
  "keywords": [
    "react",
    "react boilerplate",
    "react typescript",
    "snowpack"
  ],
  "license": "MIT",
  "main": "./src/index.tsx",
  "name": "snowpack-react-boilerplate",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git@github.com:mmykhaylov/snowpack-react-boilerplate.git"
  },
  "version": "1.0.0",
  "scripts": {
    "dev": "snowpack dev",
    "build": "snowpack build",
    "start": "servor build index.html 8080",
    "format": "prettier --config ./.prettierrc.yml --write \"./src/**/*.ts?(x)\"",
    "lint": "eslint -c ./.eslintrc.yml \"./src/**/*.ts?(x)\"",
    "postinstall": "husky install"
  },
  "devDependencies": {
    "@snowpack/plugin-react-refresh": "^2.5.0",
    "@snowpack/plugin-typescript": "^1.2.1",
    "@types/react": "^17.0.14",
    "@types/react-dom": "^17.0.9",
    "@types/snowpack-env": "^2.3.4",
    "@types/styled-components": "^5.1.11",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "husky": "^7.0.1",
    "prettier": "^2.3.2",
    "servor": "^4.0.2",
    "snowpack": "3.7.1",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "normalize.css": "^8.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-is": "^17.0.2",
    "styled-components": "^5.3.0"
  }
}
